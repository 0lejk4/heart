{
  "name": "heart-dashboard",
  "version": "0.0.1",
  "private": false,
  "homepage": ".",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint-config-prettier"
    ],
    "plugins": [
      "eslint-plugin-prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "healthcheck",
    "dashboard",
    "heart"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jap-company/heart"
  },
  "author": "Oleh Dubinskiy",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "prop-types": "^15.8.1"
  },
  "bugs": {
    "url": "https://github.com/jap-company/heart/issues"
  },
  "license": "APACHE",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__,*.md"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}